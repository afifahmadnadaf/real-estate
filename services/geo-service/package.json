{
  "name": "geo-service",
  "version": "1.0.0",
  "description": "Geographic and location service for Real Estate Platform",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "dependencies": {
    "@real-estate/common": "workspace:*",
    "@real-estate/db-models": "workspace:*",
    "axios": "^1.13.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "node-geocoder": "^4.4.1",
    "pino": "^8.17.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "js,json",
    "ignore": [
      "*.test.js"
    ]
  },
  "keywords": [
    "geo-service",
    "location",
    "geocoding",
    "real-estate"
  ],
  "author": "Real Estate Team",
  "license": "UNLICENSED"
}
